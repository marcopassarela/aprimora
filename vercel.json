{
    "version": 2,
    "builds": [
        {
            "src": "backend/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/$1"
        }
    ]
}